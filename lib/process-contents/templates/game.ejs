import React from 'react'
import BlogPost from 'components/blog/BlogPost'
import { siteUrl, authorName, authorEmail, authorTwitter } from 'site-metadata'

const SomeBlogPost = () => (
  <BlogPost
    postUrl={siteUrl + <%= JSON.stringify(attributes.slug) %>}
    postTitle={<%= JSON.stringify(attributes.title) %>}
    postDate={<%= JSON.stringify(attributes.date) %>}
    postBody={<%= JSON.stringify(body) %>}
    authorName={authorName}
    authorEmail={authorEmail}
    authorTwitter={authorTwitter}
  />
)

export default SomeBlogPost
