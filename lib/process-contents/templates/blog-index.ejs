import React from 'react'
import BlogPostExcerpts from 'components/blog/BlogPostExcerpts'

const BlogIndex = () => (
  <% const posts = files.map(({ attributes: { slug, title } }) => ({ slug, title })) %>
  <BlogPostExcerpts title={<%= JSON.stringify(attributes.title) %>} posts={<%= JSON.stringify(posts) %>} />
)

export default BlogIndex
