{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:import/errors", "plugin:prettier/recommended"],
  "rules": {
    "import/no-unresolved": "off",
    "import/order": ["error", { "newlines-between": "always" }],
    "no-console": "off"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
